<Window x:Class="JuegoReconVoz.Personalizar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Personalizar" Height="700" Width="1024" AllowsTransparency="True" WindowStartupLocation="CenterScreen" WindowStyle="None" Icon="icono DILO.ico" ResizeMode="NoResize">
    <Window.Background>
        <ImageBrush ImageSource="/JuegoReconVoz;component/Images/Frame.png" />
    </Window.Background>
    <Grid>
        <Image Height="203" HorizontalAlignment="Left" Margin="205,84,0,0" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="603" Source="/JuegoReconVoz;component/Images/sabes%20leer.png" />
        <Image Height="284" HorizontalAlignment="Left" Margin="145,293,0,0" Name="image4" Stretch="Fill" VerticalAlignment="Top" Width="244" Source="/JuegoReconVoz;component/Images/DiloLeyendo.png" />
        <Button x:Name="btnSi" Content="Button" HorizontalAlignment="Left" Height="186" Margin="442,350,0,0" Style="{DynamicResource SiEstilo}" VerticalAlignment="Top" Width="189" Click="btnSi_Click" />
        <Button x:Name="btnNo" Content="Button" HorizontalAlignment="Left" Height="186" Margin="652,350,0,0" Style="{DynamicResource NoEstilo}" VerticalAlignment="Top" Width="200" Click="btnNo_Click" />
    </Grid>
</Window>