<Window x:Class="JuegoReconVoz.SelecNivel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SelecNivel" Height="700" Width="1024" WindowStartupLocation="CenterScreen" WindowStyle="None" AllowsTransparency="True" Icon="icono DILO.ico" ResizeMode="NoResize">
    <Window.Background>
        <ImageBrush ImageSource="/JuegoReconVoz;component/Images/Frame.png" Stretch="UniformToFill" TileMode="None" />
    </Window.Background>
    <Grid>
        <Button x:Name="btnPrincip" Content="Button" HorizontalAlignment="Left" Height="150" Margin="352,147,0,0" Style="{DynamicResource PrincipianteStyle}" VerticalAlignment="Top" Width="274" Click="btnPrincip_Click" />
        <Button x:Name="btnAvanz" Content="Button" HorizontalAlignment="Left" Height="150" Margin="352,390,0,0" Style="{DynamicResource AvanzadoStyle}" VerticalAlignment="Top" Width="280" Click="btnAvanz_Click" />
        <Button x:Name="btnMini" Content="" Height="69" Margin="0,59,103,0" VerticalAlignment="Top" Style="{DynamicResource MinimizarStyle}" HorizontalAlignment="Right" Width="73" Click="btnMini_Click" />
        <Button x:Name="btnRegresar" Content="Button" HorizontalAlignment="Right" Height="88" Margin="0,55,795,0" VerticalAlignment="Top" Width="97" Style="{DynamicResource RegresarStyle}" Click="btnRegresar_Click" />
    </Grid>
</Window>
